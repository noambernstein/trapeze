<poses>
    <defaults >
        <joint name='l_hip_x' value='0' force='350' />
        <joint name='l_hip_y' value='0' force='150' />
        <joint name='l_knee' value='0' force='150' />
        <joint name='r_hip_x' value='0' force='350' />
        <joint name='r_hip_y' value='0' force='150' />
        <joint name='r_knee' value='0' force='150' />
        <joint name='l_shoulder1' value='90' force='75' />
        <joint name='l_shoulder2' value='0' force='75' />
        <joint name='l_elbow' value='0' force='75' />
        <joint name='r_shoulder1' value='90' force='75' />
        <joint name='r_shoulder2' value='0' force='75' />
        <joint name='r_elbow' value='0' force='75' />
    </defaults>

    <pose key='l' name='loose' >
        <joint name='l_hip_x' force='0' />
        <joint name='l_hip_y' force='0' />
        <joint name='l_knee' force='0' />
        <joint name='r_hip_x' force='0' />
        <joint name='r_hip_y' force='0' />
        <joint name='r_knee' force='0' />
        <joint name='l_shoulder1' force='0' />
        <joint name='l_shoulder2' force='0' />
        <joint name='l_elbow' force='0' />
        <joint name='r_shoulder1' force='0' />
        <joint name='r_shoulder2' force='0' />
        <joint name='r_elbow' force='0' />
    </pose>

    <pose key='f' name='flat' >
        <joint name='l_shoulder1' value='80' />
        <joint name='r_shoulder1' value='80' />
        <joint name='l_hip_y' value='10' />
        <joint name='r_hip_y' value='10' />
    </pose>

    <pose key='h' name='hollow' >
        <joint name='l_shoulder1' value='70' />
        <joint name='r_shoulder1' value='70' />
        <joint name='l_hip_y' value='30' />
        <joint name='r_hip_y' value='30' />
    </pose>

    <pose key='s' name='sweep' >
        <joint name='l_shoulder1' value='105' />
        <joint name='r_shoulder1' value='105' />
        <joint name='l_hip_y' value='-60' />
        <joint name='r_hip_y' value='-60' />
    </pose>

    <pose key='7' name='seven' >
        <joint name='l_shoulder1' value='-20'/>
        <joint name='r_shoulder1' value='-20'/>
    </pose>
    <pose key='8' name='seven' >
        <joint name='l_shoulder1' value='-40'/>
        <joint name='r_shoulder1' value='-40'/>
        <joint name='l_hip_y' value='20'/>
        <joint name='r_hip_y' value='20'/>
    </pose>

    <pose key='r' name='ready' >
        <joint name='l_shoulder1' value='20'/>
        <joint name='r_shoulder1' value='20'/>
        <joint name='l_hip_y' value='10' />
        <joint name='r_hip_y' value='10' />
        <joint name='l_knee' value='30' />
        <joint name='r_knee' value='30' />
    </pose>

    <pose key='c' name='curl_up' >
        <joint name='l_hip_y' value='130' />
        <joint name='r_hip_y' value='130' />
        <joint name='l_knee' value='130' />
        <joint name='r_knee' value='130' />
        <joint name='l_shoulder1' value='0' />
        <joint name='r_shoulder1' value='0' />
        <joint name='l_elbow' value='60' />
        <joint name='r_elbow' value='60' />
    </pose>

    <pose key='o' name='force_out' >
        <joint name='l_shoulder1' value='-20' />
        <joint name='r_shoulder1' value='-20' />
        <joint name='l_hip_y' value='70' />
        <joint name='r_hip_y' value='70' />
    </pose>

    <pose_sequence key='1' name='swing_front_end' >
        <pose key='s' />
        <wait time='0.6' />
        <pose key='c' />
        <wait time='0.4' />
        <pose key='o' />
        <wait time='0.1' />
        <pose key='f' />
    </pose_sequence>

    <pose_sequence key='2' name='swing_back_end' >
        <pose key='h' />
        <wait time='0.2' />
        <pose key='s' />
        <wait time='0.4' />
        <pose key='8' />
    </pose_sequence>

    <pose_sequence key='3' name='full_swing' >
        <pose key='s' />
        <wait time='0.6' />
        <pose key='c' />
        <wait time='0.4' />
        <pose key='o' />
        <wait time='0.1' />
        <pose key='f' />

        <wait time='1.1' />
        <pose key='h' />
        <wait time='0.2' />
        <pose key='s' />
        <wait time='0.4' />
        <pose key='8' />
    </pose_sequence>

    <pose_sequence key='4' name='takeoff_full_swing' >
        <pose key=' ' />
        <wait time='0.7' />
        <pose key='s' />
        <wait time='0.6' />
        <pose key='c' />
        <wait time='0.4' />
        <pose key='o' />
        <wait time='0.1' />
        <pose key='f' />

        <wait time='1.1' />
        <pose key='h' />
        <wait time='0.2' />
        <pose key='s' />
        <wait time='0.4' />
        <pose key='8' />
    </pose_sequence>

</poses>
