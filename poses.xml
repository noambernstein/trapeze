<poses>
    <defaults >
        <joint name='l_hip_x' value='0' force='200' />
        <joint name='l_hip_y' value='0' force='150' />
        <joint name='l_knee' value='0' force='150' />
        <joint name='r_hip_x' value='0' force='200' />
        <joint name='r_hip_y' value='0' force='150' />
        <joint name='r_knee' value='0' force='150' />
        <joint name='l_shoulder1' value='90' force='75' />
        <joint name='l_shoulder2' value='0' force='75' />
        <joint name='l_elbow' value='0' force='75' />
        <joint name='r_shoulder1' value='90' force='75' />
        <joint name='r_shoulder2' value='0' force='75' />
        <joint name='r_elbow' value='0' force='75' />
    </defaults>

    <pose key='l' name='loose' >
        <joint name='l_hip_x' force='0' />
        <joint name='l_hip_y' force='0' />
        <joint name='l_knee' force='0' />
        <joint name='r_hip_x' force='0' />
        <joint name='r_hip_y' force='0' />
        <joint name='r_knee' force='0' />
        <joint name='l_shoulder1' force='0' />
        <joint name='l_shoulder2' force='0' />
        <joint name='l_elbow' force='0' />
        <joint name='r_shoulder1' force='0' />
        <joint name='r_shoulder2' force='0' />
        <joint name='r_elbow' force='0' />
    </pose>

    <pose key='f' name='flat' >
        <joint name='l_shoulder1' value='80' />
        <joint name='r_shoulder1' value='80' />
        <joint name='l_hip_y' value='10' />
        <joint name='r_hip_y' value='10' />
    </pose>

    <pose key='h' name='hollow' >
        <joint name='l_shoulder1' value='70' />
        <joint name='r_shoulder1' value='70' />
        <joint name='l_hip_y' value='30' />
        <joint name='r_hip_y' value='30' />
    </pose>

    <pose key='s' name='sweep' >
        <joint name='l_shoulder1' value='105' />
        <joint name='r_shoulder1' value='105' />
        <joint name='l_hip_y' value='-60' />
        <joint name='r_hip_y' value='-60' />
    </pose>

    <pose key='7' name='seven' >
        <joint name='l_shoulder1' value='-20'/>
        <joint name='r_shoulder1' value='-20'/>
    </pose>
    <pose key='8' name='seven_hips' >
        <joint name='l_shoulder1' value='-20'/>
        <joint name='r_shoulder1' value='-20'/>
        <joint name='l_hip_y' value='10'/>
        <joint name='r_hip_y' value='10'/>
    </pose>

    <pose key='r' name='ready' >
        <joint name='l_shoulder1' value='20'/>
        <joint name='r_shoulder1' value='20'/>
        <joint name='l_hip_y' value='10' />
        <joint name='r_hip_y' value='10' />
        <joint name='l_knee' value='30' />
        <joint name='r_knee' value='30' />
    </pose>

    <pose key='c' name='curl_up' >
        <joint name='l_hip_y' value='130' />
        <joint name='r_hip_y' value='130' />
        <joint name='l_knee' value='130' />
        <joint name='r_knee' value='130' />
        <joint name='l_shoulder1' value='0' />
        <joint name='r_shoulder1' value='0' />
    </pose>

    <pose key='d' name='curl_up_elbows' >
        <joint name='l_hip_y' value='130' />
        <joint name='r_hip_y' value='130' />
        <joint name='l_knee' value='130' />
        <joint name='r_knee' value='130' />
        <joint name='l_shoulder1' value='0' />
        <joint name='r_shoulder1' value='0' />
        <joint name='l_elbow' value='60' />
        <joint name='r_elbow' value='60' />
    </pose>

    <pose key='p' name='force_out_elbows' >
        <joint name='l_shoulder1' value='-20' />
        <joint name='r_shoulder1' value='-20' />
        <joint name='l_hip_y' value='70' />
        <joint name='r_hip_y' value='70' />
        <joint name='l_elbow' value='60' />
        <joint name='r_elbow' value='60' />
    </pose>

    <pose key='o' name='force_out' >
        <joint name='l_shoulder1' value='0' />
        <joint name='r_shoulder1' value='0' />
        <joint name='l_hip_y' value='70' />
        <joint name='r_hip_y' value='70' />
    </pose>

    <action_sequence key='1' name='swing_front_end' >
        <pose key='s' />
        <wait time='0.6' />
        <pose name='curl_up' />
        <wait time='0.4' />
        <pose name='force_out' />
        <wait time='0.1' />
        <pose name='flat' />
    </action_sequence>

    <action_sequence key='2' name='swing_back_end' >
        <pose name='hollow' />
        <wait time='0.2' />
        <pose name='sweep' />
        <wait time='0.4' />
        <pose name='seven_hips' />
    </action_sequence>

    <action_sequence key='3' name='full_swing' >
        <pose name='sweep' />
        <wait time='0.6' />
        <pose name='curl_up' />
        <wait time='0.4' />
        <pose name='curl_up_elbows' />
        <wait time='0.1' />
        <pose name='force_out_elbows' />
        <wait time='0.2' />
        <pose name='force_out' />
        <wait time='0.1' />
        <pose name='flat' />

        <wait time='1.2' />
        <pose name='hollow' />
        <wait time='0.3' />
        <pose name='sweep' />
        <wait time='0.3' />
        <pose name='seven_hips' />
    </action_sequence>

    <action_sequence key='4' name='takeoff_full_swing' >
        <pose key=' ' />
        <wait time='0.9' />

        <pose name='sweep' />
        <wait time='0.6' />
        <pose name='curl_up' />
        <wait time='0.4' />
        <pose name='curl_up_elbows' />
        <wait time='0.1' />
        <pose name='force_out_elbows' />
        <wait time='0.2' />
        <pose name='force_out' />
        <wait time='0.1' />
        <pose name='flat' />

        <wait time='1.2' />
        <pose name='hollow' />
        <wait time='0.3' />
        <pose name='sweep' />
        <wait time='0.3' />
        <pose name='seven_hips' />
    </action_sequence>

    <pose key='v' name='hip_forward' >
        <joint name='l_hip_y' value='40' />
        <joint name='r_hip_y' value='40' />
        <joint name='l_shoulder1' value='45' />
        <joint name='r_shoulder1' value='45' />
    </pose>
    <pose key='b' name='hip_right' >
        <joint name='l_hip_x' value='-40' />
        <joint name='r_hip_x' value='40' />
        <joint name='l_shoulder2' value='-45' />
        <joint name='r_shoulder2' value='45' />
    </pose>
    <pose key='n' name='hip_back' >
        <joint name='l_hip_y' value='-40' />
        <joint name='r_hip_y' value='-40' />
        <joint name='l_shoulder1' value='135' />
        <joint name='r_shoulder1' value='135' />
    </pose>
    <pose key='x' name='hip_left' >
        <joint name='l_hip_x' value='40' />
        <joint name='r_hip_x' value='-40' />
        <joint name='l_shoulder2' value='45' />
        <joint name='r_shoulder2' value='-45' />
    </pose>
    <action_sequence key='m' name='miss' >
        <pose key='v' />
        <wait time='1.2' />
        <pose key='b' />
        <wait time='1.2' />
        <pose key='n' />
        <wait time='1.2' />
        <pose key='x' />
    </action_sequence>

</poses>
